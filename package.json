{
  "name": "warframe-cycle-bot",
  "version": "1.0.0",
  "description": "Discord bot to track Warframe day/night cycles across all locations",
  "main": "api/cron.js",
  "type": "module",
  "scripts": {
    "dev": "vercel dev",
    "build": "tsc",
    "deploy": "vercel --prod",
    "type-check": "tsc --noEmit",
    "check": "node dist/check-cycles.js",
    "check:local": "npm run build && npm run check"
  },
  "keywords": [
    "warframe",
    "discord",
    "bot",
    "vercel"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vercel/blob": "^0.15.1",
    "discord.js": "^14.14.1",
    "node-fetch": "^3.3.2",
    "vercel": "48.10.14"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@vercel/node": "^3.0.12",
    "typescript": "^5.3.3"
  }
}
